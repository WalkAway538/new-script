--// Auto Rob Jailbreak UI (Kavo Style) - Versi Semi Otomatis RemoteEvent // -- Pastikan kamu punya Kavo UI library di dalam game kamu

local Kavo = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/Ui-Library/main/Kavo-UI-Library.lua"))() local Window = Kavo.CreateLib("Auto Rob - Jailbreak", "DarkTheme")

local AutoRobEnabled = false local RobTargets = { Bank = true, Jewelry = true, Museum = true, CargoTrain = true, GasStation = true, DonutShop = true, OilFactory = true }

local AutoRobTab = Window:NewTab("Auto Rob") local AutoRobSection = AutoRobTab:NewSection("Control") local StatusLabel = AutoRobTab:NewSection("Log"):NewLabel("Status: Menunggu...") local MoneyLabel = AutoRobTab:NewSection("Log"):NewLabel("Uang Total: $0") local TotalMoney = 0

-- Tombol Start / Stop AutoRobSection:NewButton("Start Auto Rob", "Mulai merampok otomatis", function() AutoRobEnabled = true AutoRob() end)

AutoRobSection:NewButton("Stop Auto Rob", "Berhenti auto rob", function() AutoRobEnabled = false end)

-- Checklist lokasi aktif for name, _ in pairs(RobTargets) do AutoRobSection:NewToggle(name, "Aktifkan/Nonaktifkan "..name, function(state) RobTargets[name] = state end) end

-- Minimize Box local function CreateMinimizedUI() local ScreenGui = Instance.new("ScreenGui", game.CoreGui) local Frame = Instance.new("Frame", ScreenGui) Frame.Size = UDim2.new(0, 120, 0, 40) Frame.Position = UDim2.new(0.5, -60, 0, 0) Frame.BackgroundColor3 = Color3.fromRGB(30,30,30) Frame.BorderSizePixel = 0 Frame.AnchorPoint = Vector2.new(0.5, 0) Frame.Name = "MinimizedAutoRob" Frame.Active = true Frame.Draggable = true

local TextButton = Instance.new("TextButton", Frame)
TextButton.Size = UDim2.new(1, 0, 1, 0)
TextButton.Text = "Open Auto Rob"
TextButton.BackgroundTransparency = 1
TextButton.TextColor3 = Color3.new(1,1,1)
TextButton.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
    Kavo:ToggleUI()
end)

end

-- Tombol Minimize dan Close AutoRobSection:NewButton("Minimize UI", "Sembunyikan ke atas", function() Kavo:ToggleUI() CreateMinimizedUI() end)

AutoRobSection:NewButton("Close UI", "Tutup UI sepenuhnya", function() game.CoreGui:FindFirstChild("KavoUI"):Destroy() end)

-- Fungsi utama Auto Rob function AutoRob() spawn(function() while AutoRobEnabled do local availableRob = CheckRobbery() if availableRob and RobTargets[availableRob] then StatusLabel:UpdateLabel("Merampok: " .. availableRob) TeleportTo(availableRob) TriggerRobbery(availableRob) wait(10) TotalMoney += math.random(3000,7000) MoneyLabel:UpdateLabel("Uang Total: $" .. TotalMoney) else StatusLabel:UpdateLabel("Tidak ada lokasi rob aktif...") end wait(3) end end) end

-- Deteksi Lokasi Rob Aktif (sementara dummy transparansi) function CheckRobbery() local function isOpen(obj) return obj and obj:IsA("BasePart") and obj.Transparency > 0.5 end

if isOpen(workspace:FindFirstChild("Banks") and workspace.Banks:FindFirstChild("Door")) then
    return "Bank"
elseif isOpen(workspace:FindFirstChild("Jewelrys") and workspace.Jewelrys:FindFirstChild("Door")) then
    return "Jewelry"
elseif isOpen(workspace:FindFirstChild("GasStation") and workspace.GasStation:FindFirstChild("Door")) then
    return "GasStation"
elseif isOpen(workspace:FindFirstChild("DonutShop") and workspace.DonutShop:FindFirstChild("Door")) then
    return "DonutShop"
elseif isOpen(workspace:FindFirstChild("OilFactory") and workspace.OilFactory:FindFirstChild("Door")) then
    return "OilFactory"
end
return nil

end

-- Teleport ke lokasi function TeleportTo(tempat) local char = game.Players.LocalPlayer.Character if not char then return end local pos = { Bank = Vector3.new(10, 10, 10), Jewelry = Vector3.new(20, 10, 10), Museum = Vector3.new(30, 10, 10), CargoTrain = Vector3.new(40, 10, 10), GasStation = Vector3.new(50, 10, 10), DonutShop = Vector3.new(60, 10, 10), OilFactory = Vector3.new(70, 10, 10), } if pos[tempat] then char:MoveTo(pos[tempat]) end end

-- Trigger rob pakai RemoteEvent (semi otomatis) function TriggerRobbery(tempat) local remote = game.ReplicatedStorage:FindFirstChild("RobberyRemote") if remote and remote:IsA("RemoteEvent") then remote:FireServer(tempat) end end

-- Tab Fitur Player local PlayerTab = Window:NewTab("Player") local PlayerSection = PlayerTab:NewSection("Fitur Player")

PlayerSection:NewSlider("Speed", "Ubah kecepatan jalan", 300, 16, function(s) game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s end)

PlayerSection:NewSlider("Jump Power", "Ubah tinggi lompatan", 300, 50, function(j) game.Players.LocalPlayer.Character.Humanoid.JumpPower = j end)

PlayerSection:NewButton("Speed Hack (300)", "Langsung aktifkan speed 300", function() game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 300 end)

PlayerSection:NewButton("High Jump (300)", "Langsung aktifkan lompat tinggi", function() game.Players.LocalPlayer.Character.Humanoid.JumpPower = 300 end)

local Noclip = false PlayerSection:NewToggle("No Clip", "Lewat tembok", function(v) Noclip = v if Noclip then Stepped = game:GetService("RunService").Stepped:Connect(function() for _,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do if v:IsA("BasePart") and v.CanCollide == true then v.CanCollide = false end end end) else if Stepped then Stepped:Disconnect() end for _,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do if v:IsA("BasePart") then v.CanCollide = true end end end end)

PlayerSection:NewButton("Anti Arrest", "Cegah ditangkap polisi", function() local plr = game.Players.LocalPlayer local arrestModule = plr.Character:FindFirstChild("handcuffed") if arrestModule then arrestModule:Destroy() end plr.Character.ChildAdded:Connect(function(child) if child.Name == "handcuffed" then child:Destroy() end end) end)

PlayerSection:NewButton("Reset Speed/Jump", "Kembalikan ke default", function() local h = game.Players.LocalPlayer.Character.Humanoid h.WalkSpeed = 16 h.JumpPower = 50 end)

